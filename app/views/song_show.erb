<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%= @song.name %> by <br>
  <a href="/artists/<%= @song.artist.slug %>"> <%= @song.artist.name %></a> </h1>


<h3>List of Genres</h3>
<% @song.genres.each do |genre|%>
<a href="/genres/<%= genre.slug %>"> <%= genre.name %></a><br>
<% end %>
